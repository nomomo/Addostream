{
  "name": "addostream",
  "author": "Nomomo",
  "version": "1.59.7",
  "description": "두스트림에 새로운 기능을 추가하는 브라우저 확장 UserScript",
  "update": "",
  "notice": "",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --progress --config=webpack.config.js",
    "watch": "cross-env NODE_ENV=development webpack --watch --progress --config=webpack.config.js",
    "watch-poll": "cross-env NODE_ENV=development webpack --watch --watch-poll --progress --config=webpack.config.js",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=webpack.config.js",
    "production": "cross-env NODE_ENV=production webpack --progress --config=webpack.config.js"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "serialize-javascript": "^6.0.0",
    "to-string-loader": "^1.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^3.3.12",
    "webpack-userscript": "^2.5.8"
  },
  "dependencies": {}
}
